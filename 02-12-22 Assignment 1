#way-1:
def big(a,b):
    if a>b:
        print("a is big")
    else:
        print("b is big")
a=int(input("a="))        
b=int(input("b="))
big(a,b)

#way-2:
def big(a,b):
    if a>b:
        return 'True'
    else:
        return 'False'
a=int(input("a="))        
b=int(input("b="))
print(big(a,b))

#way-3:
def big(a,b):
if a>b:
    return True
else:
    return False
a=int(input("a="))        
b=int(input("b=")) 
if(big(a,b)):
    print("A is big")
else:
    print("B is big")


