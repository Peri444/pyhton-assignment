
NumList = [45,78,9,1,2]
print("\nElement Before Sorting is: ", NumList)
N=len(NumList)
for i in range (N):
    for j in range(i + 1, N):
        if(NumList[i] < NumList[j]):
            temp = NumList[i]
            NumList[i] = NumList[j]
            NumList[j] = temp
print("\nElement After Sorting List in Descending Order is: \n", NumList)
